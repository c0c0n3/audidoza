name:                audit-service
version:             0.1.0.0
synopsis:            Auditing of object trees.
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Andrea Falconi
-- maintainer:          
-- copyright:           
-- category:            
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable audit-service
  main-is:             Main.hs
